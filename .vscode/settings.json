  // VS Code Markdown Settings https://code.visualstudio.com/docs/languages/markdown
  "markdown.preview.scrollEditorWithPreview": true, // Sync scroll between editor and preview - Default: true
  "markdown.preview.openMarkdownLinks": "inPreview", // How to open links in markdown files - Default: "inPreview"
  "markdown.validate.enabled": true, // Enable markdown validation - Default: true
  "markdown.updateLinksOnFileMove.enabled": "prompt", // Update links when files are moved - Default: "prompt"
  "markdown.math.enabled": true, // Enable math expressions in markdown - Default: false, Recommended: true (experimental)
  "markdown.validate.fragmentLinks.enabled": "warning", // Validate fragment links in markdown - Default: "ignore", Recommended: "warning"
  "markdown.validate.fileLinks.enabled": "warning", // Validate file links in markdown - Default: "ignore", Recommended: "warning"

  // YAML Schema Validation https://code.visualstudio.com/docs/languages/yaml
  "yaml.schemas": {
    "https://raw.githubusercontent.com/compose-spec/compose-spec/master/schema/compose-spec.json": [ "**/*compose*.{yml,yaml}" ],
    "https://json.schemastore.org/github-workflow.json": [ ".github/workflows/*.{yml,yaml}" ],
    "https://json.schemastore.org/github-action.json": [ ".github/actions/**/*.{yml,yaml}", "**/action.{yml,yaml}" ],
    "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/main/schemas/v3.0/schema.json": [ "api/openapi_spec*.{yaml,yml}" ],
    "https://json.schemastore.org/kustomization.json": [ "**/kustomization.{yaml,yml}"],
    "https://json.schemastore.org/helmfile.json": [ "**/helmfile.{yaml,yml}" ],
    "https://json.schemastore.org/pre-commit-config.json": [ ".pre-commit-config.yml", ".pre-commit-config.yaml" ],
    "https://json.schemastore.org/prometheus.json": [ "prometheus.yml" ],
    "https://json.schemastore.org/grafana-dashboard.json": [ "**/grafana/dashboards/*.json" ]
  },
  "yaml.validate": false, // Disable YAML validation - Requirements.yml doesn't have a schema
  "yaml.format.enable": true, // Enable YAML formatting - Default: true
  "yaml.format.singleQuote": false, // Use double quotes in YAML - Default: false
  "yaml.format.bracketSpacing": true, // Add spaces inside brackets - Default: true
  "yaml.format.proseWrap": "preserve", // Preserve prose wrapping - Default: "preserve"

  // JSON Schema Validation https://code.visualstudio.com/docs/languages/json
  "json.schemas": [
    { "fileMatch": ["go.mod"], "url": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/go-mod.json" },
    { "fileMatch": ["Dockerfile*"], "url": "https://raw.githubusercontent.com/moby/buildkit/master/frontend/dockerfile/cmd/dockerfile-parser/schema.json" },
    { "fileMatch": ["docker-compose*.json"], "url": "https://raw.githubusercontent.com/compose-spec/compose-spec/master/schema/compose-spec.json" },
    { "fileMatch": ["pyproject.toml"], "url": "https://json.schemastore.org/pyproject.toml" },
    { "fileMatch": ["dependabot.yml", "dependabot.yaml"], "url": "https://json.schemastore.org/dependabot.json" },
    { "fileMatch": ["package.json"], "url": "https://json.schemastore.org/package.json" },
    { "fileMatch": ["tsconfig*.json"], "url": "https://json.schemastore.org/tsconfig.json" },
    { "fileMatch": ["jsconfig*.json"], "url": "https://json.schemastore.org/jsconfig.json" },
    { "fileMatch": [".eslintrc.json", ".eslintrc"], "url": "https://json.schemastore.org/eslintrc.json" },
    { "fileMatch": ["renovate.json", "renovate.json5"], "url": "https://docs.renovatebot.com/renovate-schema.json" },
    { "fileMatch": ["golangci.json"], "url": "https://golangci-lint.run/jsonschema/golangci.jsonschema.json" }
  ],

  // VS Code Editor Settings https://code.visualstudio.com/docs/getstarted/settings
  "editor.aiStats.enabled": true, // Enable AI-powered statistics in editor - Default: false, Recommended: true (experimental)
  "editor.rulers": [80, 120, 160, 200], // Show vertical rulers at specified columns - Default: [], Recommended: [80, 120]
  "editor.minimap.enabled": false, // Show minimap in editor - Default: true, Recommended: false (optional)
  "explorer.confirmDragAndDrop": false, // Confirm drag and drop operations - Default: true, Recommended: false (review)
  "explorer.confirmDelete": false, // Confirm delete operations - Default: true, Recommended: false (review)
  "terminal.integrated.enableMultiLinePasteWarning": "never", // Enable multi-line paste warning - Default: "auto", Recommended: "never" (reduce interruptions)
  "terminal.integrated.scrollback": 10000, // Terminal scrollback buffer size - Default: 1000, CHANGED: 100000 → 1000 to prevent OOM crashes
  "terminal.integrated.persistentSessionReviveProcess": "never", // Prevent terminal session revival consuming memory - Default: "onExit", ADDED for OOM prevention
  "workbench.editor.empty.hint": "hidden", // Show hint when no editor is open - Default: "welcomeView", Recommended: "hidden" (reduce clutter)

  // VS Code Chat Settings https://code.visualstudio.com/docs/copilot/chat/copilot-chat
  "chat.tools.terminal.autoApprove": {
      //
      // ─────────────────────────────────────────────────────────────
      // Mandatory safety overrides (cannot be bypassed)
      // https://code.visualstudio.com/docs/copilot/terminal#_safety-guards
      // Copilot Terminal will ALWAYS prompt for:
      //   - destructive ops (rm, del, Remove-Item)
      //   - git operations that rewrite history (reset, rebase)
      //   - PowerShell network commands (Invoke-WebRequest, Invoke-RestMethod)
      //   - privilege escalation (sudo, chmod on Linux)
      // Prefix-based allowList CANNOT override these rules.
      // ─────────────────────────────────────────────────────────────
      "git push": false,
      "git pull": false,
      "git reset": false,
      "git rebase": false,
      "rm ": false,
      "del ": false,
      "Remove-Item": false,
      "Invoke-WebRequest": false,   // Always prompts (network)
      "Invoke-RestMethod": false,   // Always prompts (network)
      "curl ": true,   // Always prompts (network)
      "wget ": true,   // Always prompts (network)

      // ─────────────────────────────────────────────────────────────
      // Project-specific scripts and commands
      // ─────────────────────────────────────────────────────────────
      ".specify/scripts/bash/": true,
      ".specify/scripts/powershell/": true,

      // ─────────────────────────────────────────────────────────────
      // SAFE COMMANDS — autoApprove allowed
      // https://code.visualstudio.com/docs/copilot/chat/copilot-chat#_terminal-commands
      // Prefix matching: "ls " matches "ls -la". Exact matches also work.
      // ─────────────────────────────────────────────────────────────

      // autoapprove wrapper (bypasses VS Code's hardcoded safety blockers)
      // Wraps commands with loopback-only network restriction for safe auto-approval
      // Reference: https://code.visualstudio.com/docs/copilot/chat/chat-tools#_terminal
      "autoapprove ": true,

      // Prefix matching (General CLI tools)
      "go ": true,
      "docker ": true,
      "git ": true,
      "code ": true,
      "pre-commit ": true,
      "golangci-lint ": true,
      "gofumpt ": true,
      "goimports ": true,
      "yamllint ": true,
      "cspell ": true,
      "jq ": true,
      "yq ": true,
      "gremlins ": true,
      "act ": true,
      "netstat": true,
      "openssl ": true,

      // Prefix matching (Java build tools)
      "gradlew ": true,
      "gradle ": true,
      "mvnw ": true,
      "mvn ": true,
      "javac ": true,
      "java ": true,

      // Prefix matching (Python tooling)
      "python ": true,
      "python3 ": true,
      "pip ": true,
      "pip3 ": true,
      "pytest ": true,

      // Prefix matching (Built-in shell commands) (safe)
      "ls ": true,
      "which ": true,
      "find ": true,
      "grep ": true,
      "cat ": true,
      "head ": true,
      "tail ": true,
      "mkdir ": true,
      "dir ": true,
      "type ": true,

      // Prefix matching (PowerShell safe cmdlets) (no network, no mutation)
      "Get-ChildItem ": true,
      "Get-Content ": true,
      "Set-Content": true,
      "New-Object": true,
      "Select-Object ": true,
      "Select-String ": true,
      "Test-Path ": true,
      "Resolve-Path": true,
      "Measure-Object ": true,
      "Format-Table ": true,
      "Format-List ": true,
      "Out-String": true,
      "Get-NetTCPConnection": true,
      "ForEach-Object": true,

      // Prefix matching (PowerShell “semi-safe” cmdlets); Allowed because they do not perform download or mutation
      "Get-Process ": true,
      "Get-Service ": true,
      "Get-Command ": true,
      "Get-Location ": true,
      "Set-Location ": true,
      "Get-Alias ": true
  },

  "chat.promptFilesRecommendations": {
    "speckit.constitution": true,
    "speckit.specify": true,
    "speckit.clarify": true,
    "speckit.plan": true,
    "speckit.tasks": true,
    "speckit.analyze": true,
    "speckit.implement": true
  },

  // Python Extension Settings https://code.visualstudio.com/docs/python/settings-reference
  "python.terminal.activateEnvironment": true, // Re-enable auto-activation for venv
  "python.defaultInterpreterPath": "python3",
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.testing.pytestArgs": ["."],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,

  // Java Extension Settings
  "maven.terminal.useJavaHome": true, // Use JAVA_HOME in Maven terminal - Default: false, Recommended: true (consistent Java version)
  "java.import.maven.enabled": true, // Enable Maven import - Default: true, Recommended: true (standard Maven support)
  "java.configuration.updateBuildConfiguration": "automatic", // Update build configuration automatically - Default: "interactive", Recommended: "automatic" (streamlined workflow)

  // ESLint Extension Settings (for JavaScript files in the project)
  "eslint.enable": true, // Enable ESLint - Default: true, Recommended: true (lint JavaScript files)
  "eslint.validate": ["javascript", "javascriptreact"], // File types to validate - Default: ["javascript", "javascriptreact"], Recommended: includes TypeScript if added later
  "eslint.workingDirectories": ["."], // Working directories for ESLint - Default: [], Recommended: project root

  // Go Extension Settings https://github.com/golang/vscode-go/blob/master/docs/settings.md
  "go.alternateTools": {
    "gopls": "gopls"
  }, // Override default tools - Default: {}, Recommended: {"gopls": "gopls"}
  "go.languageServerFlags": [
    "-rpc.trace",
    "serve",
    "--debug=localhost:6060"
  ], // Additional flags for language server - Default: [], Recommended: ["-rpc.trace", "serve", "--debug=localhost:6060"]
  // "go.formatTool": "gofumpt", // DISABLED: Let gopls handle formatting to avoid double formatting warning
  "go.lintTool": "golangci-lint", // Tool to use for linting - Default: "golint", Recommended: "golangci-lint"
  "go.vetOnSave": "off", // Run go vet on save - Default: "package", Recommended: "off"
  "go.lintOnSave": "file", // Run linter on save - Default: "package", CHANGED: "file" → "off" for OOM prevention
  "go.testOnSave": false, // Run tests on save - Default: false
  "go.coverOnSave": false, // Run coverage on save - Default: false
  "go.testExplorer.enable": true, // Enable test explorer - Default: false, Recommended: true
  "go.toolsManagement.autoUpdate": true, // Auto-update Go tools - Default: true, Recommended: true (keep tools current)
  "go.terminal.activateEnvironment": true, // Activate Go environment in terminal - Default: true
  "gopls": {
    "ui.completion.usePlaceholders": false, // Use placeholders in completion - Default: true, Recommended: false
    "ui.completion.completionBudget": "200ms", // Time budget for completion - Default: "100ms"
    "ui.diagnostic.analyses": {
      "unusedparams": true,
      "unusedvariables": true
    }, // Enable specific diagnostic analyses - Default: {}, Recommended: {"unusedparams": true, "unusedvariables": true}
    "formatting.gofumpt": true, // Use gofumpt for formatting - Default: false, Recommended: true
    "formatting.local": "cryptoutil", // Local import prefix - Default: "", Recommended: "cryptoutil"
    "buildFlags": ["-tags=e2e"], // Include files with e2e build tag in analysis - Required for e2e test files
    "ui.inlayhint.hints": {
      "assignVariableTypes": true, // Show variable types in assignments - Default: false, Recommended: true
      "compositeLiteralFields": true, // Show field names in composite literals - Default: false, Recommended: true
      "compositeLiteralTypes": true, // Show types in composite literals - Default: false, Recommended: true
      "constantValues": true, // Show constant values - Default: false, Recommended: true
      "functionTypeParameters": true, // Show function type parameters - Default: false, Recommended: true
      "parameterNames": true, // Show parameter names - Default: false, Recommended: true
      "rangeVariableTypes": true // Show range variable types - Default: false, Recommended: true
    } // Configure inlay hints - Default: {}, Recommended: all enabled
  },
  "python-envs.defaultEnvManager": "ms-python.python:system",
  "python-envs.pythonProjects": [] // gopls language server configuration - Default: {}, Recommended: configured settings
}
