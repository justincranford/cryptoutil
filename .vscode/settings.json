    "**/*.pyd": true,
    "**/.Python": true,
    "**/pip-log.txt": true,
    "**/pip-delete-this-directory.txt": true,
    "**/.venv": true,
    "**/venv": true,
    "**/env": true,
    "**/ENV": true,
    "**/.tox": true,
    "**/.coverage": true,
    "**/.coverage.*": true,
    "**/*.cover": true,
    "**/.hypothesis": true,
    "**/.pytest_cache": true,
    "**/.mypy_cache": true,
