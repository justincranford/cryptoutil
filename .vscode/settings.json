    "**/.coverage": true,
    "**/.coverage.*": true,
    "**/coverage.xml": true,
    "**/*.cover": true,
    "**/.hypothesis/**": true,
    "**/.pytest_cache/**": true,
    "**/.mypy_cache/**": true,
    "**/.nox/**": true,
    "**/htmlcov/**": true,
    "**/.python-version": true,
    "**/Pipfile.lock": true,
    "**/poetry.lock": true,
    "**/__pypackages__/**": true,
    "**/.ruff_cache/**": true,
    "**/.dmypy.json": true,
    "**/pyrightconfig.json": true,
    "**/.pdm.toml": true,
    "**/.pdm-python": true
