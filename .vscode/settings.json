{
  "$schema": "vscode://schemas/settings/user",
  // VS Code Copilot Customization Instructions https://code.visualstudio.com/docs/copilot/customization/custom-instructions
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  }, // Enable Copilot for all languages by default (workspace-level). Caution: confirm policy.
  "github.copilot.chat.codeGeneration.useInstructionFiles": true, // Use instruction files for code generation - Default: false, Recommended: true
  "github.copilot.chat.generateTests.codeLens": true, // Enable test generation code lens - Default: false, Recommended: true (experimental)
  "github.copilot.chat.editor.temporalContext.enabled": true, // Enable temporal context in editor - Default: false, Recommended: true (experimental)
  "github.copilot.nextEditSuggestions.enabled": true, // Enable next edit suggestions - Default: false, Recommended: true (experimental)
  "chat.agentSessionsViewLocation": "view", // Agent sessions view location - Default: "auxiliary", Recommended: "view" (main view)
  "chat.agent.maxRequests": 1000, // Maximum agent requests - Default: 25, Recommended: 1000 (higher limit for complex tasks)
  "chat.math.enabled": true, // Enable math expressions in chat - Default: false, Recommended: true (experimental)
  "chat.fontSize": 14, // Increase GitHub Copilot Chat font size for non-code text (default 12)
  "chat.editor.fontSize": 14, // Increase GitHub Copilot Chat font size for code text (default 12)
  "workbench.secondarySideBar.defaultVisibility": "visible", // Default visibility of secondary sidebar - Default: "hidden", Recommended: "visible"

  "git.enableSmartCommit": true, // Commit button is only enabled when you have staged changes - Default: true
  "git.confirmSync": false, // Confirm git sync operations - Default: true, Recommended: false (optional)
  "git.openRepositoryInParentFolders": "always", // Open git repository in parent folders - Default: "never", Recommended: "always" (enables nested repo support)
  "git.blame.editorDecoration.enabled": true, // Show git blame information inline in the editor

  // VS Code Terminal Settings https://code.visualstudio.com/docs/terminal/profiles
  "terminal.integrated.fontSize": 15, // Increase terminal font size (default 12)
  "terminal.integrated.shellIntegration.enabled": true, // Enable shell integration for better command detection
  "terminal.integrated.shellIntegration.history": 500,
  "terminal.integrated.defaultProfile.windows": "PowerShell Core", // Set PowerShell Core (pwsh) as default terminal profile
  "terminal.integrated.profiles.windows": {
    "PowerShell Core": {
      "source": "PowerShell",
      "icon": "terminal-powershell",
      "args": ["-NoLogo"]
    },
    "PowerShell": {
      "source": "PowerShell",
      "args": ["-NoLogo", "-ExecutionPolicy", "RemoteSigned"]
    }
  }, // Configure PowerShell Core (pwsh) terminal for shell integration support; fallback to Windows PowerShell 5.1 if needed

    // Diff Editor Settings
  "diffEditor.ignoreTrimWhitespace": false, // Ignore trimmed whitespace in diff - Default: true
  "diffEditor.hideUnchangedRegions.enabled": false, // Hide unchanged regions in diff - Default: false

  // VS Code Files Settings https://code.visualstudio.com/docs/getstarted/settings
  "files.encoding": "utf8", // File encoding to use - Default: "utf8"
  "files.insertFinalNewline": true, // Ensure files end with a newline - Recommended to align with project policy
  "files.trimFinalNewlines": true, // Trim extra final newlines - Recommended for consistent file endings
  "files.associations": { "*.py": "python" },
  "files.trimTrailingWhitespace": true, // Trim trailing whitespace on save - Default: false, Recommended: true (aligns with linting standards)
  "files.exclude": {
    // Git internal files
    "**/.git": false,
    "**/.git/COMMIT_EDITMSG": true,
    "**/.git/description": true,
    "**/.git/info": true,
    "**/.git/index": true,
    "**/.git/refs": true,
    "**/.git/packed-refs": true,
    "**/.git/logs": true,
    "**/.git/lost-found": true,
    "**/.git/objects": true,
    "**/.git/FETCH_HEAD": true,
    "**/.git/ORIG_HEAD": true,
    "**/.git/HEAD": true,

    // System/OS files
    "**/.DS_Store": true,
    "**/Thumbs.db": true,

    // Temporary files
    "**/*.tmp": true,
    "**/*.swp": true,
    "**/*.swo": true,

    // Build artifacts
    "**/bin": true,
    "**/*.exe": true,
    "**/*.exe~": true,
    "**/*.dll": true,
    "**/*.so": true,
    "**/*.dylib": true,

    // Java/Maven/Gradle build artifacts
    "**/out": true,

    // Maven/Gradle caches
    "**/.mvn": true,
    "**/.gradle": true,

    // Go workspace files
    "**/go.work": true,
    "**/go.work.sum": true,

    // Dependencies
    "**/vendor": true,
    "**/node_modules": true,

    // Cache files and cicd outputs
    "**/.cspellcache": true,
    "**/.cicd": true,

    // Environment files
    "**/.env": true,

    // Test artifacts - CRITICAL for OOM prevention
    "**/test-output/**": true,
    "**/*.test": true,
    "**/coverage": true,
    "**/coverage_*": true,
    "**/coverage*.out": true,
    "**/coverage*.txt": true,
    "**/coverage.xml": true,
    "**/coverage.html": true,
    "**/coverage.json": true,
    "**/coverage.lcov": true,
    "**/orm_coverage": true,
    "**/*_coverage": true,
    "**/*_functions.txt": true,
    "**/test-results": true,

    // Go build cache - CRITICAL for OOM prevention
    "**/*.test.exe": true,
    "**/*.cover.go": true,
    "**/covervars.go": true,

    // Python artifacts
    "**/__pycache__": true,
    "**/*.pyc": true,
    "**/*.pyo": true,
    "**/*.pyd": true,
    "**/.Python": true,
    "**/pip-log.txt": true,
    "**/pip-delete-this-directory.txt": true,
    "**/.venv": true,
    "**/venv": true,
    "**/env": true,
    "**/ENV": true,
    "**/.tox": true,
    "**/.coverage": true,
    "**/.coverage.*": true,
    "**/*.cover": true,
    "**/.hypothesis": true,
    "**/.pytest_cache": true,
    "**/.mypy_cache": true,
    "**/.nox": true,
    "**/htmlcov": true,
    "**/.python-version": true,
    "**/Pipfile.lock": true,
    "**/poetry.lock": true,
    "**/__pypackages__": true,
    "**/.ruff_cache": true,
    "**/.dmypy.json": true,
    "**/pyrightconfig.json": true,
    "**/.pdm.toml": true,
    "**/.pdm-python": true
  },
