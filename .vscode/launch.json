{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

         {
            "name": "cryptoutil sqlite",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "--dev=true",
                "--log-level=INFO",
            ],
        },
        {
            "name": "cryptoutil postgres",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "--database-url=postgres://USR:PWD@localhost:5432/DB?sslmode=disable",
                "--log-level=ALL",
            ],
        },
        {
            "name": "cryptoutil model",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/../oapi-codegen/cmd/oapi-codegen/oapi-codegen.go",
            "cwd": "${workspaceFolder}/api",
            "args": [
                "--config=./openapi_gen_model.yaml",
                "./openapi_spec_components.yaml"
            ]
        },
        {
            "name": "cryptoutil client",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/../oapi-codegen/cmd/oapi-codegen/oapi-codegen.go",
            "cwd": "${workspaceFolder}/api",
            "args": [
                "--config=./openapi_gen_client.yaml",
                "./openapi_spec_paths.yaml"
            ]
        },
        {
            "name": "cryptoutil server",
            "type": "go",
            "request": "launch",
            "program": "${workspaceFolder}/../oapi-codegen/cmd/oapi-codegen/oapi-codegen.go",
            "cwd": "${workspaceFolder}/api",
            "args": [
                "--config=./openapi_gen_server.yaml",
                "./openapi_spec_paths.yaml"
            ]
        }
    ]
}
